! Fonts
Xft.dpi:        120
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.autohint:  true
Xft.hintstyle:  hintfull

URxvt.font: xft:Monospace:pixelsize=14:Regular
URxvt.boldFont: xft:Monospace:pixelsize=14:Bold
URxvt.talicFont: xft:Monospace:pixelsize=14:Italic
URxvt.bolditalicFont: xft:Monospace:pixelsize=14:Bold:Italic

urxvt.xftAutoHint: true
urxvt.realBold: True
urxvt.scrollBar: False
urxvt.internalBorder: 3
urxvt.externalBorder: 0
urxvt.saveLines: 10000
urxvt.cursorBlink: true

URxvt*background: #000000
URxvt*foreground: #ffffff
! black
URxvt.color0  : #000000
URxvt.color8  : #555555
! red
URxvt.color1  : #AA0000
URxvt.color9  : #FF5555
! green
URxvt.color2  : #00AA00
URxvt.color10 : #55FF55
! yellow
URxvt.color3  : #AA5500
URxvt.color11 : #FFFF55
! blue
URxvt.color4  : #0000AA
URxvt.color12 : #5555FF
! magenta
URxvt.color5  : #AA00AA
URxvt.color13 : #FF55FF
! cyan
URxvt.color6  : #00AAAA
URxvt.color14 : #55FFFF
! white
URxvt.color7  : #AAAAAA
URxvt.color15 : #FFFFFF

! URxvt*letterSpace: -1
URxvt.matcher.button  : 1
URxvt.perl-ext-common : default,matcher
URxvt.urlLauncher     : chromium-browser

*XTerm*background: black
*XTerm*foreground: gray
*XTerm*renderFont: true
*XTerm*faceName: Droid\ Sans\ Mono
*XTerm*faceSize: 11
